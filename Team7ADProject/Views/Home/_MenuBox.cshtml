@model Team7ADProject.ViewModels.SideBarViewModel
<script>
    $("menuBox").click(function () {
        window.location = $(this).find("a").attr("href");
        return false;
    })

</script>

<div>
    <div class="menuBox">
        <div style="padding-left:25%; padding-top:67%;">
            <a class="custlink" href="@Url.Action(Model.Action,Model.Controller)" style="text-align:center;"> <b>@Html.Raw(Model.Title)</b></a>
        </div>
    </div>
</div>